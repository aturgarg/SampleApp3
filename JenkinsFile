pipeline {
environment {
    registry = “gustavoapolinario/docker-test”
    registryCredential = ‘dockerhub’
    dockerImage = ‘’
  }
agent any
stages {
stage ('Checkout from git') {
            steps {
                 git url: 'https://github.com/aturgarg/SampleApp3',branch: 'master'
            }
}
stage ('build docker') {     
         steps {
             sh "sudo docker build -t sampleapp3_a ."
          }
        }
stage('docker run image') {
      steps {
            sh 'sudo docker run -d -p 19003:80 --name sampleapp3_a1_1 sampleapp3_a1'
       }
    }
stage(‘Deploy Image’) {
  steps{
    script {
      docker.withRegistry( ‘’, registryCredential ) {
        dockerImage.push()
      }
    }
  }
}
 }
}
