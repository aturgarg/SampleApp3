pipeline {
agent any
//environment {
//    dotnet = 'path\\to\\dotnet.exe'
//}
stages {
stage ('Checkout') {
            steps {
                 git url: 'https://github.com/aturgarg/SampleApp3',branch: 'master'
            }
}
stage ('build docker') {     
         steps {
             sh "docker build -t sampleapp3_a ."
          }
        }
stage('docker run image') {
      steps {
            sh 'docker run -d -p 19003:80 --name sampleapp3_a1_1 sampleapp3_a1'
       }
    }
 }
}
